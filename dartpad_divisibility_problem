void main() {
  for (int i = 1; i <= 100; i++){
    if (i%3 == 0 && i%5 == 0) {
      print ('BudVue should consider Samuel for this position');
    }
    else if (i%5 == 0) {
      print ('Vue');
    }
    else if (i%3 == 0) {
      print ('Bud');
    }
    else {
      print ('${i}');
    }
  }
}
